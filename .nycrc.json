{
  "extends": "@istanbuljs/nyc-config-typescript",
  "cache": false,
  "extension": [".ts"],
  "check-coverage": true,
  "include": ["src/**"],
  "statements": 90,
  "branches": 90,
  "functions": 95,
  "lines": 90,
  "all": true,
  "reporter": [
    "text",
    "html",
    "text-summary",
    "lcov"
  ]
}
